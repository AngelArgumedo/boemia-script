// While loop
let counter: int = 0;
while counter < 5 {
    print(counter);
    counter = counter + 1;
}

// For loop
for i: int = 1; i <= 10; i = i + 1 {
    print(i);
}

// Nested loop
for x: int = 1; x <= 3; x = x + 1 {
    for y: int = 1; y <= 3; y = y + 1 {
        let product: int = x * y;
        print(product);
    }
}
