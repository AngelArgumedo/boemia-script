// Member access
let cantidad: int = numeros.length;

// Method calls
numeros.push(60);
numeros.push(70);

// Arrays anidados con índices encadenados
let matrix: [[int]] = [[1, 2, 3], [4, 5, 6], [7, 8, 9]];
let valor: int = matrix[0][1];
let otro: int = matrix[1][2];

// Combinar operaciones
let ultimo_idx: int = numeros.length;
nombres.push("Diana");

print(primero);
print(cantidad);
print(valor);
